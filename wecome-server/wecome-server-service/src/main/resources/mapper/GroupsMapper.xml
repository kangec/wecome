<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kangec.wecome.infrastructure.mapper.GroupsMapper">
    <select id="queryGroupsById" resultType="Groups" parameterType="java.lang.String">
        SELECT * FROM `groups` WHERE group_id = #{ groupId }
    </select>

</mapper>